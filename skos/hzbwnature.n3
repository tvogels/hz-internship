  skos:related <http://wiki.local/index.php/Bodem> .

<http://wiki.local/index.php/Inlaatsluis> rdf:type skos:Concept ;
  skos:prefLabel "Inlaatsluis" ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Kunstwerk> .

<http://wiki.local/index.php/Innundatiesluis> rdf:type skos:Concept ;
  skos:prefLabel "Innundatiesluis" ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Kunstwerk> .

<http://wiki.local/index.php/Inscharingslengte> rdf:type skos:Concept ;
