  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Overstromingskans> ;
  skos:related <http://wiki.local/index.php/Invloedsgebied> ;
  skos:related <http://wiki.local/index.php/Macrostabiliteit> ;
  skos:related <http://wiki.local/index.php/Bres> ;
  skos:related <http://wiki.local/index.php/Calamiteit> ;
  skos:related <http://wiki.local/index.php/Waterkering> ;
  skos:related <http://wiki.local/index.php/Golfoverslagdebiet> ;
  skos:related <http://wiki.local/index.php/Schadefactor> ;
  skos:related <http://wiki.local/index.php/Parti%C3%ABle_factor> ;
  skos:partOf <http://wiki.local/index.php/Ramp> .

<http://wiki.local/index.php/Aanleghoogte> rdf:type skos:Concept ;
  skos:prefLabel "Aanleghoogte" ;
  skos:definition "Kruinhoogte van de waterkering onmiddellijk na het gereedkomen ervan." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:partOf <http://wiki.local/index.php/Kruin> .

<http://wiki.local/index.php/Aansluitingsconstructie> rdf:type skos:Concept ;
  skos:prefLabel "Aansluitingsconstructie" ;
  skos:definition "Gehele dwars- en lengteprofiel van een grondconstructie in zijn afwijkende vorm, bij de overgang naar een duin, hoge gronden of een kunstwerk." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:partOf <http://wiki.local/index.php/Duin> ;
  skos:partOf <http://wiki.local/index.php/Hoge_gronden> ;
  skos:partOf <http://wiki.local/index.php/Kunstwerk> .

<http://wiki.local/index.php/Achterland> rdf:type skos:Concept ;
  skos:prefLabel "Achterland" ;
  skos:definition "Het gebied aansluitend aan de landzijde van de waterkering." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Binnendijks> ;
  skos:related <http://wiki.local/index.php/Kwelkade> .

<http://wiki.local/index.php/Achterloopsheid> rdf:type skos:Concept ;
  skos:prefLabel "Achterloopsheid" ;
  skos:definition "Lekstroom achter een constructie om." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Faalmechanisme> .

<http://wiki.local/index.php/Actuele_sterkte> rdf:type skos:Concept ;
  skos:prefLabel "Actuele sterkte" ;
  skos:definition "Huidige werkelijke sterkte." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Waterkering> .

<http://wiki.local/index.php/Afschuiven> rdf:type skos:Concept ;
  skos:prefLabel "Afschuiven" ;
  skos:definition "Verplaatsen van een deel van een grondlichaam door overschrijding van het evenwichtsdraagvermogen." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> .

<http://wiki.local/index.php/Afschuiven_voorland> rdf:type skos:Concept ;
  skos:prefLabel "Afschuiven voorland" ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Inscharingslengte> ;
  skos:broader <http://wiki.local/index.php/Faalmechanisme> .

<http://wiki.local/index.php/Afslaglijn> rdf:type skos:Concept ;
  skos:prefLabel "Afslaglijn" ;
  skos:definition "Lijn in lengterichting van de kust die de afslagpunten verbindt." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Afslagpunt> .

<http://wiki.local/index.php/Afslagprofiel> rdf:type skos:Concept ;
  skos:prefLabel "Afslagprofiel" ;
  skos:definition "1. Evenwichtsprofiel van een duin tijdens de maatgevende storm. 2. Resterende profiel van een hoog voorland, inclusief dijk, na aanzienlijke buitendijkse erosie." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Duinafslag> .

<http://wiki.local/index.php/Afslagpunt> rdf:type skos:Concept ;
  skos:prefLabel "Afslagpunt" ;
  skos:definition "Snijpunt van het afgeslagen duinfront met het Rekenpeil. Het kritieke afslagpunt geeft die mate van duinafslag aan waarbij nog juist geen doorbreken optreedt." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Afslaglijn> ;
  skos:partOf <http://wiki.local/index.php/Afslagprofiel> .

<http://wiki.local/index.php/Aquifer> rdf:type skos:Concept ;
  skos:prefLabel "Aquifer" ;
  skos:definition "Watervoerende zandlaag." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Grenslaag> .

<http://wiki.local/index.php/Artesisch_water> rdf:type skos:Concept ;
  skos:prefLabel "Artesisch water" ;
  skos:definition "(Grond)water met een wateroverspanning ten opzichte van een hydrostatische waterspanningsverdeling waarbij de wateroverspanning het gevolg is van de stijghoogte in het eerste watervoerend pakket." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Grondwater> ;
  skos:partOf <http://wiki.local/index.php/Aquifer> .

<http://wiki.local/index.php/Asfalt> rdf:type skos:Concept ;
  skos:prefLabel "Asfalt" .

<http://wiki.local/index.php/Astronomisch_getij> rdf:type skos:Concept ;
  skos:prefLabel "Astronomisch getij" ;
  skos:definition "Getijbeweging als gevolg van de veranderlijke resultante van de aantrekkingskracht van de maan en de zon op de watermassaâ€™s op aarde, niet gestoord door weerkundige omstandigheden." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Waterstand> ;
  skos:related <http://wiki.local/index.php/Zeespiegelstijging> .

<http://wiki.local/index.php/Bandijk> rdf:type skos:Concept ;
  skos:prefLabel "Bandijk" ;
  skos:definition "Rivierdijk die het winterbed omsluit." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Waterkering> ;
  skos:partOf <http://wiki.local/index.php/Rivierengebied> .

<http://wiki.local/index.php/Basiskustlijn> rdf:type skos:Concept ;
  skos:prefLabel "Basiskustlijn" ;
  skos:definition "Kustlijn die in het kader van de kustlijnzorg zal worden gehandhaafd. In het algemeen de positie van de gemiddelde kustlijn op 1 januari 1990." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Kustlijn> ;
  skos:related <http://wiki.local/index.php/Suppletie> ;
  skos:related <http://wiki.local/index.php/Kustbeheer> .

<http://wiki.local/index.php/Basispeil> rdf:type skos:Concept ;
  skos:prefLabel "Basispeil" ;
  skos:definition "Extreme hoogwaterstand met (per definitie) een overschrijdingsfrequentie van 1/10.000 per jaar." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Veiligheidsnorm> .

<http://wiki.local/index.php/Beheer> rdf:type skos:Concept ;
  skos:prefLabel "Beheer" ;
  skos:definition "Geheel van activiteiten dat noodzakelijk is om te waarborgen dat de functies van de waterkering blijven voldoen aan de daarvoor vastgestelde eisen en normen." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Waterkering> .

<http://wiki.local/index.php/Beheerder> rdf:type skos:Concept ;
  skos:prefLabel "Beheerder" ;
  skos:definition "Overheid waarbij de (primaire) waterkering in beheer is." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Beheersgebied> ;
  skos:partOf <http://wiki.local/index.php/Beheer> .

<http://wiki.local/index.php/Beheersgebied> rdf:type skos:Concept ;
  skos:prefLabel "Beheersgebied" ;
  skos:definition "In de legger gespecificeerd areaal, dat als waterkering wordt aangemerkt en door de waterkeringbeheerder wordt beheerd." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Beheerder> .

<http://wiki.local/index.php/Beheersregister> rdf:type skos:Concept ;
  skos:prefLabel "Beheersregister" ;
  skos:definition "Document met de beschrijving van de feitelijke toestand van de waterkering, met de voor het behoud van het waterkerend vermogen kenmerkende gegevens van de constructie." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Legger> ;
  skos:related <http://wiki.local/index.php/Waterkering> .

<http://wiki.local/index.php/Belasting> rdf:type skos:Concept ;
  skos:prefLabel "Belasting" ;
  skos:definition "Op een constructie (een waterkering) uitgeoefende in- en uitwendige krachten, ofwel de mate waarin een constructie door in- en uitwendige krachten wordt aangesproken, uitgedrukt in een fysische grootheid." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Veiligheidstoeslag> ;
  skos:related <http://wiki.local/index.php/Ontwerp> ;
  skos:related <http://wiki.local/index.php/Parti%C3%ABle_factor> ;
  skos:related <http://wiki.local/index.php/Stabiliteitsfactor> .

<http://wiki.local/index.php/Belastinggevallenmethode> rdf:type skos:Concept ;
  skos:prefLabel "Belastinggevallenmethode" ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Ontwerp> .

<http://wiki.local/index.php/Benedenrivierengebied> rdf:type skos:Concept ;
  skos:prefLabel "Benedenrivierengebied" ;
  skos:definition "(1)Door Rijn en Maas gevoede rivierengebied, waarbij tijdens grote afvoergolven de waterstanden een significante invloed ondervinden van de waterstand op de Noordzee als gevolg van een zware storm De getijhoogwaterstijging is hier van belang. (2)Het rivierengebied ten westen van de lijn Schoonhoven -Werkendam Dongemond, inclusief Hollands Diep en Haringvliet, maar zonder de Hollandsche IJssel. De combinatie van waterstanden op zee en rivierafvoeren veroorzaakt in dit gebied de hoge waterstanden." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Rivierengebied> .

<http://wiki.local/index.php/Benedenstrooms> rdf:type skos:Concept ;
  skos:prefLabel "Benedenstrooms" ;
  skos:definition "Stroomafwaarts." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Stroomgebied> .

<http://wiki.local/index.php/Beoordelingsprofiel> rdf:type skos:Concept ;
  skos:prefLabel "Beoordelingsprofiel" ;
  skos:definition "Denkbeeldig minimum profiel van gedefinieerde afmetingen dat binnen het werkelijk aanwezige profiel van een dijk moet passen. Dit profiel wordt gebruikt ten behoeve van het beoordelen van de veiligheid van bestaande dijken op de aanwezigheid van niet-waterkerende objecten. Het mag in het algemeen niet worden doorsneden door verstoringzones van niet-waterkerende objecten." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Dijkbekleding> .

<http://wiki.local/index.php/Beschermingsniveau> rdf:type skos:Concept ;
  skos:prefLabel "Beschermingsniveau" ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Veiligheidsnorm> .

<http://wiki.local/index.php/Beschermingszone> rdf:type skos:Concept ;
  skos:prefLabel "Beschermingszone" ;
  skos:definition "(1)In de legger aangegeven beheerszone ter weerszijde van de waterkering. (2)Stroken grond aan weerszijden van de kernzone, die bijdragen aan de stabiliteit van de waterkering." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:partOf <http://wiki.local/index.php/Legger> ;
  skos:partOf <http://wiki.local/index.php/Waterkering> .

<http://wiki.local/index.php/Bestemmingsplan> rdf:type skos:Concept ;
  skos:prefLabel "Bestemmingsplan" ;
  skos:definition "Een plan dat beschrijft wat er met de ruimte in een bepaalde gemeente mag gebeuren." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Ruimte_voor_de_Rivier> .

<http://wiki.local/index.php/Betonzuil> rdf:type skos:Concept ;
  skos:prefLabel "Betonzuil" ;
  skos:definition "Waterkerend kunstwerk Constructie die onderdeel uitmaakt van een waterkering of de waterkering vervangt, maar is aangelegd ten behoeve van een andere functie, die de waterkering kruist (bijvoorbeeld schutten, spuien) 1)" .

<http://wiki.local/index.php/Betrekkingslijn> rdf:type skos:Concept ;
  skos:prefLabel "Betrekkingslijn" ;
  skos:definition "Lijn die weergeeft welke waterstanden met gelijke overschrijdingsfrequentie aan twee peilstations met elkaar overeenkomen." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Overschrijdingsfrequentie> ;
  skos:related <http://wiki.local/index.php/Waterstand> .

<http://wiki.local/index.php/Big_Picture> rdf:type skos:Concept ;
  skos:prefLabel "Big Picture" ;
  skos:broader <http://wiki.local/index.php/EMontPatronen> .

<http://wiki.local/index.php/Binnendijks> rdf:type skos:Concept ;
  skos:prefLabel "Binnendijks" ;
  skos:definition "(1)Aan de kant van het land of het binnenwater. (2)Gebied landwaarts van de waterkering waarvoor een wettelijke veiligheidsnorm is gedefinieerd. De landwaartse grens van de waterkering is de grens met het dijkringgebied. Ook wel de grens tussen binnendijks en buitendijks genoemd." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Achterland> ;
  skos:related <http://wiki.local/index.php/Waterkering> .

<http://wiki.local/index.php/Binnentalud> rdf:type skos:Concept ;
  skos:prefLabel "Binnentalud" ;
  skos:definition "(1)Hellend vlak van het dijklichaam aan de binnendijkse zijde van de dijk. (2)Het schuin aflopende deel aan de landzijde van de dijk." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Talud> .

<http://wiki.local/index.php/Binnenteen> rdf:type skos:Concept ;
  skos:prefLabel "Binnenteen" ;
  skos:definition "Onderrand van het dijklichaam aan de binnendijkse zijde van de dijk (de overgang van dijk naar maaiveld)." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:partOf <http://wiki.local/index.php/Binnentalud> .

<http://wiki.local/index.php/Binnenwater> rdf:type skos:Concept ;
  skos:prefLabel "Binnenwater" ;
  skos:definition "De binnenwateren van een land bestaan uit alle wateren aan de landzijde van de basislijn. Dit ter onderscheid van de zee. De binnenwateren zijn in twee groepen te verdelen: *rivieren, kanalen en meren die voor binnenvaartschepen bevaarbaar zijn (meestal aangeduid als vaarweg); *alle maritieme wateren achter de basislijn: baaien, mondingen en zeehavens." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Oppervlaktewater> .

<http://wiki.local/index.php/Bochtwerking> rdf:type skos:Concept ;
  skos:prefLabel "Bochtwerking" ;
  skos:definition "Waterstandverhoging ter plaatse van de waterkering als gevolg van scheefstand van het wateroppervlak in een bocht van een rivier" ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Waterstand> ;
  skos:related <http://wiki.local/index.php/Rivier> .

<http://wiki.local/index.php/Bodem> rdf:type skos:Concept ;
  skos:prefLabel "Bodem" ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Grondwater> ;
  skos:related <http://wiki.local/index.php/Infitratie> .

<http://wiki.local/index.php/Bovenrivierengebied> rdf:type skos:Concept ;
  skos:prefLabel "Bovenrivierengebied" ;
  skos:definition "(1)Door Rijn en Maas gevoede rivierengebied (inclusief de IJssel), waarbij de waterstanden geen significante invloed ondervinden van de waterstand op de Noordzee en het IJsselmeer als gevolg van een zware storm. De getijhoogwaterstijging speelt hier geen rol. (2)Het door Rijn en Maas gevoede rivierengebied ten oosten van de lijn Schoonhoven - Werkendam â€“ Dongemond. Hoge rivierafvoer veroorzaakt de hoge waterstanden in dit gebied." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Rivierengebied> .

<http://wiki.local/index.php/Bovenstrooms> rdf:type skos:Concept ;
  skos:prefLabel "Bovenstrooms" ;
  skos:definition "Stroomopwaarts." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Stroomgebied> .

<http://wiki.local/index.php/Bres> rdf:type skos:Concept ;
  skos:prefLabel "Bres" ;
  skos:definition "Gat in de waterkering." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/(Het)_Bezwijken> .

<http://wiki.local/index.php/Buioscillaties> rdf:type skos:Concept ;
  skos:prefLabel "Buioscillaties" ;
  skos:definition "Onregelmatige schommelingen van het wateroppervlak met een wisselende periode die vooral bij zware storm optreden." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Kruinhoogtemarge> ;
  skos:related <http://wiki.local/index.php/Waterstand> .

<http://wiki.local/index.php/Buistoot> rdf:type skos:Concept ;
  skos:prefLabel "Buistoot" ;
  skos:definition "Afzonderlijk optredende vrij kort durende waterspiegelverheffing als gevolg van een zware bui." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Buioscillaties> .

<http://wiki.local/index.php/Buitenberm> rdf:type skos:Concept ;
  skos:prefLabel "Buitenberm" ;
  skos:definition "Extra verbreding aan de buitendijkse zijde van de dijk om het dijklichaam extra steun te bieden, om zandmeevoerende wellen te voorkomen en/of om de golfoploop te reduceren." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:partOf <http://wiki.local/index.php/Buitentalud> .

<http://wiki.local/index.php/Buitendijks> rdf:type skos:Concept ;
  skos:prefLabel "Buitendijks" ;
  skos:definition "- *Aan de kant van het te keren (buiten)water 1) *Gebied zeewaarts van de waterkering waarvoor geen wettelijke veiligheidsnorm is gedefinieerd. De landwaartse grens van de waterkering is de grens met het dijkringgebied. Ook wel de grens tussen binnendijks en buitendijks genoemd. 2)" ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Voorland> ;
  skos:related <http://wiki.local/index.php/Waterkering> .

<http://wiki.local/index.php/Buitenkruinlijn> rdf:type skos:Concept ;
  skos:prefLabel "Buitenkruinlijn" ;
  skos:definition "Lijn die de overgang markeert tussen de kruin en het buitentalud, waarlangs de toetsing op hoogte plaatsvindt." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:partOf <http://wiki.local/index.php/Buitentalud> ;
  skos:partOf <http://wiki.local/index.php/Kruin> .

<http://wiki.local/index.php/Buitentalud> rdf:type skos:Concept ;
  skos:prefLabel "Buitentalud" ;
  skos:definition "(1)Hellend vlak van het dijklichaam aan de kerende zijde. (2)Het schuin aflopende deel aan de rivierzijde van de dijk." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:broader <http://wiki.local/index.php/Talud> .

<http://wiki.local/index.php/Buitenteen> rdf:type skos:Concept ;
  skos:prefLabel "Buitenteen" ;
  skos:definition "Onderrand van het dijklichaam aan de buitendijkse zijde van de dijk (de overgang van dijk naar maaiveld en/of voorland)." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:partOf <http://wiki.local/index.php/Buitentalud> .

<http://wiki.local/index.php/Buitenwater> rdf:type skos:Concept ;
  skos:prefLabel "Buitenwater" ;
  skos:definition "Oppervlaktewater waarvan de waterstand direct onder invloed staat van de waterstand op zee, de grote rivieren, het IJsselmeer of het Markermeer." ;
  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Intreepunt> ;
  skos:related <http://wiki.local/index.php/Normfrequentie> ;
  skos:related <http://wiki.local/index.php/Primaire_waterkering> ;
  skos:broader <http://wiki.local/index.php/Oppervlaktewater> .

