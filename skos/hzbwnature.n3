  skos:related <http://wiki.local/index.php/Afschuiven_voorland> .

<http://wiki.local/index.php/Intergetijdegebied> rdf:type skos:Concept ;
  skos:prefLabel "Intergetijdegebied" ;
  skos:definition "Dit is het gebied dat bij laag water droog komt te liggen en bij hoogwater overstroomt." ;
