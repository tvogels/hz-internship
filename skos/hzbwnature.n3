  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/(Het)_Bezwijken> ;
  skos:related <http://wiki.local/index.php/HIS_(Hoogwater_Informatie_Systeem)> ;
  skos:related <http://wiki.local/index.php/Dijkringbenadering> .

<http://wiki.local/index.php/Calamiteitenplan> rdf:type skos:Concept ;
  skos:prefLabel "Calamiteitenplan" ;
  skos:definition "Een draaiboek waarin verschillende acties om de dijk te bewaken (in geval van calamiteit) staan vermeld. Volgens de Waterstaatswet 1900 zijn waterbeheerders verplicht dit op te stellen." ;
