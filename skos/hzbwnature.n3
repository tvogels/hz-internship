  skos:inScheme <http://wiki.local/index.php/Waterveiligheid> ;
  skos:related <http://wiki.local/index.php/Buitenwater> ;
  skos:partOf <http://wiki.local/index.php/Waterstand> .

<http://wiki.local/index.php/Ondergrond> rdf:type skos:Concept ;
